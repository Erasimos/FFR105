function [newIndividual1 newIndividual2] = TwoPointCrossover(Individual1, Individual2)
    
    chromosome1 = Individual1.Chromosome;
    chromosome2 = Individual2.Chromosome;
    
    nGenes = size(chromosome1, 1)/4;
    
    % Crossover only happens between instructions
    crossOverPoint1 = (1 + fix(rand*nGenes))*4;
    crossOverPoint2 = (1 + fix(rand*nGenes))*4;
    
    newChromosome1 = [chromosome1(0:crossOverPoint1) cross]
end 